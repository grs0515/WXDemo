/* util/xtabbar/tabbar-component.wxss */
/* 整个的tabbar的高是80rpx  */
.tabbar-component {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin: 5rpx 3rpx 0rpx 0rpx;
}

.tabbar-parent {
  display: flex;
  flex-direction: column;
  align-items: center
}

.tabbar-image {
  height: 50rpx;
  width: 50rpx;
}
.tabbar-text{
  font-size: 22rpx;
}
